f (!this.form.validCode) {\n            ElMessage.error(\"请填写验证码\")\n            return\n          }\n          if(this.form.validCode.toLowerCase() !== this.validCode.toLowerCase()) {\n            ElMessage.error(\"验证码错误\")\n            return\n          }\n          if(this.form.password != this.form.confirm)\n          {\n            ElMessage.error(\"两次密码输入不一致\")\n            return\n          }\n          if(this.form.role == 0 && this.form.authorize != \"2236\")\n          {\n            ElMessage.error(\"请输入正确的注册码\")\n            return\n          }\n          if(this.form.role == 1 && this.form.authorize != \"zuozhe\")\n          {\n            ElMessage.error(\"请输入正确的作者邀请码\")\n            return\n          }\n          request.post(\"user/register\",this.form).then(res=>{\n            if(res.code == 200 || res.code == 0)\n            {\n              ElMessage.success(\"注册成功\")\n              this.$router.push(\"/login\")\n            }\n            else {ElMessage.error(res.msg)}\n          })\n        }\n      })\n\n    }\n  }\n\n  }\n\n</script>\n\n<style scoped>\n.login-container {\n  position: fixed;\n  width: 100%;\n  height: 100%;\n  background: url('../../img/bg2.svg');\n  background-size: contain;\n}\n.login-page {\n  border-radius: 5px;\n  margin: 180px auto;\n  width: 350px;\n  padding: 35px 35px 15px;\n  background: #fff;\n  border: 1px solid #eaeaea;\n  box-shadow: 0 0 25px #cac6c6;\n}\n</style>"],"mappings":";;;;EACMA,KAAK,EAAC;AAAiB;gEAEvBC,mBAAA,CAAuD;EAAnDD,KAAK,EAAC,OAAO;EAACE,KAA2B,EAA3B;IAAA;EAAA;GAA4B,MAAI;;EA0C3CA,KAAqB,EAArB;IAAA;EAAA;AAAqB;;;;;;;;;;;uBA5ClCC,mBAAA,CAsDM,OAtDNC,UAsDM,GArDFC,YAAA,CAoDUC,kBAAA;IApDDC,GAAG,EAAC,MAAM;IAAEC,KAAK,EAAEC,KAAA,CAAAC,IAAI;IAAMC,KAAK,EAAEF,KAAA,CAAAE,KAAK;IAAEX,KAAK,EAAC;;sBACxD,MAAuD,CAAvDY,UAAuD,EACvDP,YAAA,CAMeQ,uBAAA;MANDC,IAAI,EAAC;IAAU;wBAC3B,MAIW,CAJXT,YAAA,CAIWU,mBAAA;oBAJQN,KAAA,CAAAC,IAAI,CAACM,QAAQ;mEAAbP,KAAA,CAAAC,IAAI,CAACM,QAAQ,GAAAC,MAAA;QAAEC,WAAW,EAAC,QAAQ;QAACC,SAAS,EAAT;;QAC1